<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://tera-arise.io/dc/VillagerDialog VillagerDialog.xsd"
                id="1001"
                huntingZoneId="518"
                voiceTypeId="47"
                xmlns="https://tera-arise.io/dc/VillagerDialog">
    <Text id="1"
          endSocial="14">
        <Page social="4"
              voiceTypeId="0">Building trust among the races is paramount. We cannot erase the past, but if we are going to fight for unity, we must believe in it.</Page>
    </Text>
    <Text id="101"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">I trust in you to get it done.</Page>
    </Text>
    <Text id="102"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">What?</Page>
    </Text>
    <Text id="103"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Wasn't Hellen already dead?</Page>
    </Text>
    <Text id="104"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Such a tragedy.</Page>
    </Text>
    <Text id="105"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">They were outstanding members of the Federation.</Page>
    </Text>
    <Text id="106"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">I will organize a memorial service to honor their sacrifice.</Page>
    </Text>
</VillagerDialog>
