<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="https://tera-arise.io/dc/VillagerDialog VillagerDialog.xsd"
                id="10401"
                huntingZoneId="1"
                voiceTypeId="0"
                xmlns="https://tera-arise.io/dc/VillagerDialog">
    <Text id="1"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Thank you for your help.</Page>
    </Text>
    <Text id="101"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Thanks for helping our tribe.</Page>
    </Text>
    <Text id="102"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">You should avoid the other Shurians as they're still poisoned.</Page>
    </Text>
    <Text id="103"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">I won't be any help to you as I'm from the same tribe.</Page>
    </Text>
    <Text id="104"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Head to the North Gate.</Page>
    </Text>
    <Text id="105"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Akasha deceived us and poisoned the spring.</Page>
    </Text>
    <Text id="106"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">We thought we would gain great power from the Spring of Arun.</Page>
    </Text>
    <Text id="107"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">But quite the opposite: it has brought ruin to my tribe.</Page>
    </Text>
</VillagerDialog>
