<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://vezel.dev/novadrop/dc/VillagerDialog VillagerDialog.xsd" id="40011000" huntingZoneId="808" voiceTypeId="95" xmlns="https://vezel.dev/novadrop/dc/VillagerDialog">
    <Text id="101" endSocial="0">
        <Page social="0" voiceTypeId="0">You think only guards are at my disposal?</Page>
    </Text>
    <Text id="102" endSocial="0">
        <Page social="0" voiceTypeId="0">&lt;font color='#FF0000'&gt;Feel the piercing pain!&lt;/font&gt;</Page>
    </Text>
    <Text id="103" endSocial="0">
        <Page social="0" voiceTypeId="0">&lt;font color='#FF0000'&gt;Taste the power of blood magic!&lt;/font&gt;</Page>
    </Text>
    <Text id="104" endSocial="0">
        <Page social="0" voiceTypeId="0">&lt;font color='#FF0000'&gt;My blood drainers will finish you!&lt;/font&gt;</Page>
    </Text>
    <Text id="105" endSocial="0">
        <Page social="0" voiceTypeId="0">Enough playing around.</Page>
    </Text>
    <Text id="106" endSocial="0">
        <Page social="0" voiceTypeId="0">What a surprise. I'm impressed…  but not enough to let you live.</Page>
    </Text>
    <Text id="107" endSocial="0">
        <Page social="0" voiceTypeId="0">How dare you! Kill! Kill them all!</Page>
    </Text>
    <Text id="108" endSocial="0">
        <Page social="0" voiceTypeId="0">&lt;font color='#FF0000'&gt;You shouldn't have come!&lt;/font&gt;</Page>
    </Text>
    <Text id="109" endSocial="0">
        <Page social="0" voiceTypeId="0">&lt;font color='#FF0000'&gt;This will be your grave!&lt;/font&gt;</Page>
    </Text>
    <Text id="110" endSocial="0">
        <Page social="0" voiceTypeId="0">To pass, you just need to pay… with your life!</Page>
    </Text>
</VillagerDialog>
