    <Text id="1"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">It's my fault... My friends only died because I am so weak.</Page>
    </Text>
    <Text id="101"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Fraya, I need to tell you something.</Page>
    </Text>
    <Text id="102"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">Hasmina and Master Sorcerer Hellen are dead.</Page>
    </Text>
    <Text id="103"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">They were true Arborean heroes.</Page>
    </Text>
    <Text id="104"
          endSocial="0">
        <Page social="0"
              voiceTypeId="0">I will arrange a memorial service at once.</Page>
    </Text>
</VillagerDialog>
