<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://vezel.dev/novadrop/dc/VillagerDialog VillagerDialog.xsd" id="5038" huntingZoneId="528" voiceTypeId="58" xmlns="https://vezel.dev/novadrop/dc/VillagerDialog">
    <Text id="1" endSocial="3">
        <Page social="5" voiceTypeId="58">&lt;PCNAME&gt;, we will use your example for all federation soldiers.</Page>
    </Text>
    <Text id="101" endSocial="0">
        <Page social="0" voiceTypeId="0">We just received news from the front!</Page>
    </Text>
    <Text id="102" endSocial="0">
        <Page social="0" voiceTypeId="0">Welcome back, &lt;PCNAME&gt;.</Page>
    </Text>
    <Text id="103" endSocial="0">
        <Page social="0" voiceTypeId="0">Timely, my friend, timely. We must talk.</Page>
    </Text>
    <Text id="104" endSocial="0">
        <Page social="0" voiceTypeId="0">I'm just going over the reports from the front.</Page>
    </Text>
    <Text id="105" endSocial="0">
        <Page social="0" voiceTypeId="0">We need more information to locate Shandra Manaya.</Page>
    </Text>
    <Text id="106" endSocial="0">
        <Page social="0" voiceTypeId="0">I want you to accompany me.</Page>
    </Text>
    <Text id="110" endSocial="0">
        <Page social="0" voiceTypeId="0">Help them. I know you will end the war.</Page>
    </Text>
</VillagerDialog>
