<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://vezel.dev/novadrop/dc/VillagerDialog VillagerDialog.xsd" id="100" huntingZoneId="2002" voiceTypeId="324" xmlns="https://vezel.dev/novadrop/dc/VillagerDialog">
    <Text id="1" endSocial="1">
        <Page social="2" voiceTypeId="324">Oh, Mother...&lt;BR&gt;&lt;BR&gt;A stranger...? Here...?</Page>
    </Text>
    <Text id="101" endSocial="1">
        <Page social="2" voiceTypeId="324">You're strangers...</Page>
    </Text>
    <Text id="102" endSocial="1">
        <Page social="2" voiceTypeId="324">My ally is waiting to help find a cure.</Page>
    </Text>
    <Text id="103" endSocial="1">
        <Page social="2" voiceTypeId="324">Basheek...might know.</Page>
    </Text>
    <Text id="104" endSocial="1">
        <Page social="2" voiceTypeId="324">I haven't seen your companion.</Page>
    </Text>
    <Text id="105" endSocial="1">
        <Page social="2" voiceTypeId="324">Follow the path, and you'll find someone to guide you.</Page>
    </Text>
</VillagerDialog>
