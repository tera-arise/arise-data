<VillagerDialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://vezel.dev/novadrop/dc/VillagerDialog VillagerDialog.xsd" id="10100" huntingZoneId="2503" voiceTypeId="0" xmlns="https://vezel.dev/novadrop/dc/VillagerDialog">
    <Text id="101" endSocial="0">
        <Page social="0" voiceTypeId="0">Hahaha, you fool.</Page>
    </Text>
    <Text id="102" endSocial="0">
        <Page social="0" voiceTypeId="0">You have absolutely no idea what’s going on.</Page>
    </Text>
    <Text id="103" endSocial="0">
        <Page social="0" voiceTypeId="0">You’ve been fooled around by my illusion till now.</Page>
    </Text>
    <Text id="104" endSocial="0">
        <Page social="0" voiceTypeId="0">Here, take a look at my companion and my assistant, alive and well, and definitely not the subject of your experiments.</Page>
    </Text>
    <Text id="105" endSocial="0">
        <Page social="0" voiceTypeId="0">And I am Paesyn, the Hero of the Federation!</Page>
    </Text>
    <Text id="106" endSocial="0">
        <Page social="0" voiceTypeId="0">Uwaaaa!</Page>
    </Text>
    <Text id="107" endSocial="0">
        <Page social="0" voiceTypeId="0">Now, the lovely ladies are all gone.</Page>
    </Text>
    <Text id="108" endSocial="0">
        <Page social="0" voiceTypeId="0">It’s time for your punishment!</Page>
    </Text>
</VillagerDialog>
