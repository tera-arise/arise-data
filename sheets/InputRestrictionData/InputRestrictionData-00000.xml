    <Enabled server="false"
             client="true" />
    <RegularExpressions>
        <RegularExpression name="CharacterName">
            <Check target="Alphabet" />
            <Check target="Lower-Alphabet"
                   repeat="true"
                   if="Upper-Alphabet-ascii"
                   replace="Lower-Alphabet-ascii" />
            <Check max="1"
                   target="AddSpecialChar"
                   repeat="true"
                   if="Space"
                   replace="Punctuation">
                <Check target="Alphabet" />
                <Check target="Lower-Alphabet"
                       repeat="true"
                       if="Upper-Alphabet-ascii"
                       replace="Lower-Alphabet-ascii" />
            </Check>
        </RegularExpression>
        <RegularExpression name="GuildName">
            <Check target="Alphabet" />
            <Check target="Lower-Alphabet"
                   repeat="true" />
            <Check target="Space"
                   repeat="true">
                <Check target="Alphabet" />
                <Check target="Lower-Alphabet"
                       repeat="true" />
            </Check>
        </RegularExpression>
    </RegularExpressions>
